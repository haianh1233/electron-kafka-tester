{
  "name": "electron-kafka-tester",
  "version": "1.1.22",
  "description": "electron-kafka-tester",
  "main": "main.js",
  "scripts": {
    "start": "concurrently -k \"npx vite --config vite.config.js\" \"wait-on http://localhost:3000 && electron-forge start\"",
    "start-demo": "concurrently -k \"npx vite --config vite.config.js\" \"wait-on http://localhost:3000 && cross-env START_DEMO=true electron-forge start\"",
    "test": "echo \\\"Error: no backend specified\\\" && exit 1",
    "build:backend": "cd demo && mvn clean package && mkdir -p ../backend && cp target/demo-0.0.1-SNAPSHOT.jar ../backend",
    "build:frontend": "vite build --config vite.config.js",
    "package": "npm run build:frontend && electron-forge package",
    "make": "npm run build:frontend && electron-forge make",
    "publish": "npm run build:frontend && electron-forge publish",
    "dev-make": "npm run build:backend && npm run make",
    "dev-package": "npm run build:backend && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haianh1233/electron-kafka-tester.git"
  },
  "author": "anhnguyen",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.0.1",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@vitejs/plugin-react": "^4.3.3",
    "antd": "^5.22.0",
    "axios": "^1.7.7",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "electron-is-dev": "^3.0.1",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.1",
    "kafkajs": "^2.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "update-electron-app": "^3.0.0",
    "vite": "^5.4.11"
  }
}
